Algoritmo SEC04_JOEL
	Definir A, B, Suma, Resta, Multi, Div Como Real
	
	Escribir "Ingrese el primer número: "
	Leer A
	Escribir "Ingrese el segundo número: "
	Leer B
	
	Suma <- A + B
	Resta <- A - B
	Multi <- A * B
	Div <- A / B
	
	Escribir "Suma: ", Suma
	Escribir "Resta: ", Resta
	Escribir "Multiplicación: ", Multi
	Escribir "División: ", Div
FinAlgoritmo
